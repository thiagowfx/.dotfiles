# keep-sorted start newline_separated=yes
[submodule "nvim/.config/nvim/lazy.nvim"]
	path = nvim/.config/nvim/lazy.nvim
	url = https://github.com/folke/lazy.nvim.git
	shallow = true

[submodule "vendor/ghostty-cursor-shaders"]
	path = vendor/ghostty-cursor-shaders
	url = https://github.com/sahaj-b/ghostty-cursor-shaders
	shallow = true

[submodule "vendor/grml-etc-core"]
	path = vendor/grml-etc-core
	url = https://github.com/grml/grml-etc-core
	shallow = true

[submodule "vim/.vim/vim-plug"]
	path = vim/.vim/vim-plug
	url = https://github.com/junegunn/vim-plug.git
	shallow = true

[submodule "zsh/.zsh/zsh-autosuggestions"]
	path = zsh/.zsh/zsh-autosuggestions
	url = https://github.com/zsh-users/zsh-autosuggestions
	shallow = true

[submodule "zsh/.zsh/zsh-defer"]
	path = zsh/.zsh/zsh-defer
	url = https://github.com/romkatv/zsh-defer.git
	shallow = true

[submodule "zsh/.zsh/zsh-syntax-highlighting"]
	path = zsh/.zsh/zsh-syntax-highlighting
	url = https://github.com/zsh-users/zsh-syntax-highlighting
	shallow = true
# keep-sorted end
