{
  "magic": "nassh-prefs",
  "version": 1,
  "nassh": {
    "profile-ids": [
      {
        "id": "b44c",
        "json": {
          "description": "user@host",
          "username": "user",
          "hostname": "host",
          "argstr": "-t -- tmux new -A -s main"
        }
      }
    ]
  },
  "hterm": {
    "default": {
      "alt-backspace-is-meta-backspace": true,
      "background-color": "rgba(39, 40, 34, 1)",
      "color-palette-overrides": [
        "#272822",
        "#f92672",
        "#a6e22e",
        "#f4bf75",
        "#66d9ef",
        "#ae81ff",
        "#a1efe4",
        "#f8f8f2",
        "#75715e",
        "#f92672",
        "#a6e22e",
        "#f4bf75",
        "#66d9ef",
        "#ae81ff",
        "#a1efe4",
        "#f9f8f5"
      ],
      "font-family": "\"Fira Code\", \"PT Mono\", \"DejaVu Sans Mono\", \"Noto Sans Mono\", \"Everson Mono\", FreeMono, Menlo, Terminal, monospace",
      "font-size": "12",
      "foreground-color": "rgba(197, 200, 198, 1)",
      "scroll-on-output": true,
      "user-css": "https://fonts.googleapis.com/css2?family=Fira+Code:wght@400;700&display=swap",
