Host example
    HostName example.com
    User root
    IdentityFile ~/.ssh/id_rsa

Host *
    ServerAliveInterval 60
    ServerAliveCountMax 300

    Compression yes
    AddKeysToAgent yes

    # macOS keychain
    UseKeychain yes

    # IdentityFile ~/.ssh/id_rsa

# vim: ft=sshconfig fdm=marker sw=4 ts=4 et
