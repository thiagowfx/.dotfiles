Host example
    HostName example.com
    User root
    IdentityFile ~/.ssh/id_rsa

Host *
    ServerAliveInterval 60
    ServerAliveCountMax 300

    Compression yes
    CompressionLevel 6

    AddKeysToAgent yes

    # macOS keychain
    # UseKeychain yes

